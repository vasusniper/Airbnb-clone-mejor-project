<% if(success&&success.length){%>
    <div class="row">
    <div class="col-8 offset-2 alert alert-success alert-dismissible fade show" role="alert">
        <%=success%>
        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="close">

        </button>
    </div>
 </div>
<% } %>
<% if(error&&error.length){%>
    <div class="row">
    <div class="col-8 offset-2 alert alert-danger alert-dismissible fade show" role="alert">
        <%=error%>
        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="close">

        </button>
    </div>
 </div>
<% } %>